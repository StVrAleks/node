<script>
    allDataDB();
async function allDataDB(){
let allFlowersRows;
const localToken = localStorage.getItem('floweridaKey');
let allTitleGoods = document.getElementsByClassName('title_goods');
let allFullTitle = document.getElementsByClassName('full_title');
let allprice = document.getElementsByClassName('price');
let allimg = document.querySelectorAll('.imgs_goods > img');
let flowersId = document.getElementsByClassName('discriptionId');
let allSection = document.getElementsByClassName('conteiner_section');
let allView = document.getElementsByClassName('watch');

for(var i=0; i<allSection.length; i++)
    document.getElementsByClassName('conteiner_section')[i].style.display = 'none';

fetch('/api/flower/getAll',{
        method: "GET",
        headers: {"content-Type": "application/json"},
       // body: JSON.stringify({'flowerId':flowerId})
        })
        .then((response) => response.json())
        .then(data =>{
           if(data.mes || data.message)
             document.getElementById('mist10').innerHTML = data.mes || data.message;
           if(data.rows.length > 0)
              {
                for(var i = 0; i < data.rows.length; i++) //data.rows.length
                 {
                  flowersId[i].innerHTML = data.rows[i]['flowerVidId'];  
                  allFullTitle[i].innerHTML = data.rows[i]['name'];
                  allprice[i].innerHTML = data.rows[i]['price'] + ' руб.';
                  allView[i].id = data.rows[i]['id'];
                  allSection[i].style.display = 'block';
                  vidOne(data.rows[i]['flowerVidId'], i);
                  imgOne(data.rows[i]['id'], i);
                  document.getElementById(data.rows[i]['id']).addEventListener('click', event => {oneItem(event);});
                 }
               }
             
        });   
}

async function vidOne(id, num){
let allTitleGoods = document.getElementsByClassName('title_goods');
  fetch('/api/vid/getOne',{
      method: "POST",
      headers: {"content-Type": "application/json"},
      body: JSON.stringify({'id':id})
      })
      .then((response) => response.json())
      .then(data =>{
        if(data.mes || data.message)
            console.log(data.mes);
        else
        {
            allTitleGoods[num].innerHTML=data['name'];
        }  
       });
}

async function imgOne(id, num){
let allimg = document.querySelectorAll('.imgs_goods > img');
  fetch('/api/imgs/getAll',{
      method: "POST",
      headers: {"content-Type": "application/json"},
      body: JSON.stringify({'flowerId':id})
      })
      .then((response) => response.json())
      .then(data =>{
        if(data.mes || data.message)
            console.log(data.mes);
        else if(data.rows.length > 0)
        {
            let path = '/imgStoreMINI/' + data.rows[0]['img'];
            allimg[num].src=path;
        }  
       });    
}
function oneItem(event){
 let trId = event.target.id;  
return  window.location.href = '/flower.html?id='+trId; 

}
